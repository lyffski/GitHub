{
  "storybookUrl": "https://www.lusini.com/de-de/extensions/storybook/",
  "defaultStory": "base/Button",
  "mediaSizes": [
    {
      "label": "PHONE",
      "key": "XS",
      "icon": "MOBILE",
      "gap": 10,
      "breakpoint": 0,
      "initialyActive": true
    },
    {
      "label": "PHONE LANDSCAPE",
      "key": "SM",
      "icon": "TABLET",
      "gap": 15,
      "breakpoint": 576,
      "initialyActive": false
    },
    {
      "label": "TABLET",
      "key": "MD",
      "icon": "LAPTOP",
      "gap": 15,
      "breakpoint": 768,
      "initialyActive": false
    },
    {
      "label": "DESKTOP",
      "key": "LG",
      "icon": "DESKTOP",
      "gap": 20,
      "breakpoint": 992,
      "initialyActive": false
    },
    {
      "label": "DESKTOP LARGE",
      "key": "XL",
      "icon": "DESKTOP",
      "gap": 20,
      "breakpoint": 1200,
      "initialyActive": false
    }
  ],
  "connector": "@kaminrunde/fireside-connector-contentful",
  "plugins": [
    {
      "resolve": "fireside-plugin-component-gap",
      "options": {
        "key": "componentGap"
      }
    }
  ]
}
